@{
    ViewBag.Title = "OW车辆管理系统";
    Layout = "~/Views/Shared/_Template.cshtml";
}
@using OW.Operation.OW.Model
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>OW 车辆管理系统</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="robots" content="all,follow">

</head>
<body>
    <div class="page">

        <div class="page-content d-flex align-items-stretch">
            <!-- 侧边 -->
            @Html.Partial("_sidebar")
            <!--侧边-->
            <div class="content-inner">
                <!-- Page Header-->
                <header class="page-header">
                    <div class="container-fluid">
                        <h2 class="no-margin-bottom">系统数据显示</h2>
                    </div>
                </header>
                <!-- Dashboard Counts Section-->
                <section class="dashboard-counts no-padding-bottom">
                    <div class="container-fluid">
                        <div class="row bg-white has-shadow">
                            <!-- Item -->
                            <div class="col-xl-3 col-sm-6">
                                <div class="item d-flex align-items-center">
                                    <div class="icon bg-violet"><i class="icon-user"></i></div>
                                    <div class="title">
                                        <span>会员<br>车辆</span>
                                        <div class="progress">
                                            <div role="progressbar" style="width: @ViewBag.hcar%; height: 4px;" aria-valuenow="@ViewBag.hcar" aria-valuemin="0" aria-valuemax="100" class="progress-bar bg-violet"></div>
                                        </div>
                                    </div>
                                    <div class="number"><strong>@ViewBag.hcar</strong></div>
                                </div>
                            </div>
                            <!-- Item -->
                            <div class="col-xl-3 col-sm-6">
                                <div class="item d-flex align-items-center">
                                    <div class="icon bg-red"><i class="icon-padnote"></i></div>
                                    <div class="title">
                                        <span>外来<br>车辆</span>
                                        <div class="progress">
                                            <div role="progressbar" style="width: @ViewBag.ccar%; height: 4px;" aria-valuenow="@ViewBag.ccar" aria-valuemin="0" aria-valuemax="100" class="progress-bar bg-red"></div>
                                        </div>
                                    </div>
                                    <div class="number"><strong>@ViewBag.ccar</strong></div>
                                </div>
                            </div>
                            <!-- Item -->
                            <div class="col-xl-3 col-sm-6">
                                <div class="item d-flex align-items-center">
                                    <div class="icon bg-green"><i class="icon-bill"></i></div>
                                    <div class="title">
                                        <span>充值<br>车辆</span>
                                        <div class="progress">
                                            <div role="progressbar" style="width: @ViewBag.wcar%; height: 4px;" aria-valuenow="@ViewBag.wcar" aria-valuemin="0" aria-valuemax="100" class="progress-bar bg-green"></div>
                                        </div>
                                    </div>
                                    <div class="number"><strong>@ViewBag.wcar</strong></div>
                                </div>
                            </div>
                            <!-- Item -->
                            <div class="col-xl-3 col-sm-6">
                                <div class="item d-flex align-items-center">
                                    <div class="icon bg-orange"><i class="icon-check"></i></div>
                                    <div class="title">
                                        <span>剩余<br>库位</span>
                                        <div class="progress">
                                            <div role="progressbar" style="width: @ViewBag.pcount%; height: 4px;" aria-valuenow="@ViewBag.pcount" aria-valuemin="0" aria-valuemax="100" class="progress-bar bg-orange"></div>
                                        </div>
                                    </div>
                                    <div class="number"><strong>@ViewBag.pcount</strong></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <!-- Dashboard Header Section    -->
                <section class="dashboard-header">
                    <div class="container-fluid">
                        <div class="row">
                            <!-- Statistics -->
                            <div class="statistics col-lg-3 col-12">
                                <div class="statistic d-flex align-items-center bg-white has-shadow">
                                    <div class="icon bg-red"><i class="fa fa-tasks"></i></div>
                                    <div class="text"><strong>234</strong><br><small>Applications</small></div>
                                </div>
                                <div class="statistic d-flex align-items-center bg-white has-shadow">
                                    <div class="icon bg-green"><i class="fa fa-calendar-o"></i></div>
                                    <div class="text"><strong>152</strong><br><small>Interviews</small></div>
                                </div>
                                <div class="statistic d-flex align-items-center bg-white has-shadow">
                                    <div class="icon bg-orange"><i class="fa fa-paper-plane-o"></i></div>
                                    <div class="text"><strong>147</strong><br><small>Forwards</small></div>
                                </div>
                            </div>
                            <!-- 车库位置图-->
                            <div class="chart col-lg-6 col-12">
                                <div class="line-chart bg-white d-flex align-items-center justify-content-center has-shadow">
                                    @Html.Partial("Svgdisplay")
                                </div>
                            </div>
                            <div class="chart col-lg-3 col-12">
                                <!-- 车库位置图  -->
                                <div class="bar-chart has-shadow bg-white" style="position:relative;">
                                    @*<div class="title"><strong class="text-violet">95%</strong><br><small>当前系统正常运行时间</small></div>*@
                                    <div id="system" class="system" style="width:100%;height:300px">

                                    </div>
                                </div>
                                <!-- Numbers-->
                                <div class="statistic d-flex align-items-center bg-white has-shadow">
                                    <div class="icon bg-green"><i class="fa fa-line-chart"></i></div>
                                    <div class="text"><strong>99.9%</strong><br><small>成功率</small></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <!-- Projects Section-->
                <section class="projects no-padding-top">
                    <div class="container-fluid">

                        @foreach (CarSettledInfo item in ViewBag.scared)
                        {
                            <!-- Project-->
                            <div class="project">
                                <div class="row bg-white has-shadow">
                                    <div class="left-col col-lg-6 d-flex align-items-center justify-content-between">
                                        <div class="project-title d-flex align-items-center">
                                            <div class="image has-shadow"><img src="~/wwwroot/comm/img/project-1.jpg" alt="..." class="img-fluid"></div>

                                            <div class="text">
                                                <h3 class="h4">车牌号：@item.License</h3><small>@item.Brand</small>
                                            </div>
                                        </div>
                                        <div class="project-date"><span class="hidden-sm-down">@item.SettledDate</span></div>
                                    </div>
                                    <div class="right-col col-lg-6 d-flex align-items-center">
                                        <div class="time"><i class="fa fa-clock-o"></i>6月28日12:00出库</div>
                                        <div class="comments"><i class="fa fa-comment-o"></i>会员车：本次停车免费</div>

                                    </div>
                                </div>
                            </div>
                            <!-- Project-->
                        }







                        @*<div class="project">
                            <div class="row bg-white has-shadow">
                                <div class="left-col col-lg-6 d-flex align-items-center justify-content-between">
                                    <div class="project-title d-flex align-items-center">
                                        <div class="image has-shadow"><img src="~/wwwroot/comm/img/project-2.jpg" alt="..." class="img-fluid"></div>
                                        <div class="text">
                                            <h3 class="h4">车牌号：桂A·686868</h3><small>保时捷</small>
                                        </div>
                                    </div>
                                    <div class="project-date"><span class="hidden-sm-down">6月27凌晨4点24分入库</span></div>
                                </div>
                                <div class="right-col col-lg-6 d-flex align-items-center">
                                    <div class="time"><i class="fa fa-clock-o"></i>6月28日12:00出库</div>
                                    <div class="comments"><i class="fa fa-comment-o"></i>充值车：本次停车免费</div>

                                </div>
                            </div>
                        </div>
                        <!-- Project-->
                        <div class="project">
                            <div class="row bg-white has-shadow">
                                <div class="left-col col-lg-6 d-flex align-items-center justify-content-between">
                                    <div class="project-title d-flex align-items-center">
                                        <div class="image has-shadow"><img src="~/wwwroot/comm/img/project-3.jpg" alt="..." class="img-fluid"></div>
                                        <div class="text">
                                            <h3 class="h4">车牌号：桂A·1C3456</h3><small>凯迪拉克</small>
                                        </div>
                                    </div>
                                    <div class="project-date"><span class="hidden-sm-down">6月27凌晨4点24分入库</span></div>
                                </div>
                                <div class="right-col col-lg-6 d-flex align-items-center">
                                    <div class="time"><i class="fa fa-clock-o"></i>6月28日12:00出库</div>
                                    <div class="comments"><i class="fa fa-comment-o"></i>外来车辆：本次停车费用50元</div>

                                </div>
