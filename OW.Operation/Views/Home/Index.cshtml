                        </div>
                        <!-- Project-->
                        <div class="project">
                            <div class="row bg-white has-shadow">
                                <div class="left-col col-lg-6 d-flex align-items-center justify-content-between">
                                    <div class="project-title d-flex align-items-center">
                                        <div class="image has-shadow"><img src="~/wwwroot/comm/img/project-4.jpg" alt="..." class="img-fluid"></div>
                                        <div class="text">
                                            <h3 class="h4">车牌号：桂A·7A8D98</h3><small>未知车型</small>
                                        </div>
                                    </div>
                                    <div class="project-date"><span class="hidden-sm-down">6月27凌晨4点24分入库</span></div>
                                </div>
                                <div class="right-col col-lg-6 d-flex align-items-center">
                                    <div class="time"><i class="fa fa-clock-o"></i>6月28日12:00出库</div>
                                    <div class="comments"><i class="fa fa-comment-o"></i>外来车辆：本次停车费用50元</div>

                                </div>
                            </div>
                        </div>
                    

                        //////*@
                    </div>
                </section>

            </div>
        </div>
    </div>
    <script src="~/wwwroot/comm/echarts.js"></script>
    <script>       
        var myChart = echarts.init(document.getElementById('system'));
        option =@Html.Raw(Session["option"]);
        myChart.setOption(option);
    </script>
</body>
</html>
